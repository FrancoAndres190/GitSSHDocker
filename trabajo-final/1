  springboot_app:
    container_name: springboot_app
    image: springboot_app_template
    restart: always
    build:
      context: ./springboot_app
    ports: "8000:8080"
    depends_on:
      - mysqldb
  mysqldb:
    container-name: mysqldb
    image: "mysql:latest"
    restart: always
    ports:
      - 3306:3306
    environment:
      MYSQL_DATABASE: tasks_gestor
      MYSQL_PASSWORD: root
      MYSQL_USER: root
      MYSQL_ROOT_PASSWORD: root
